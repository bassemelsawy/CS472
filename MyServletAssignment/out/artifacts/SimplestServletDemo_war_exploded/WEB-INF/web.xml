<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SimplestServlet</servlet-name>
        <servlet-class>mum.cs472.myServlet.SimplestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>mum.cs472.myServlet.HelloWorld</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BeerSelect</servlet-name>
        <servlet-class>mum.cs472.myServlet.BeerSelect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calculator</servlet-name>
        <servlet-class>mum.cs472.calc.CalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Quiz</servlet-name>
        <servlet-class>mum.cs472.Quiz.QuizServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SimplestServlet</servlet-name>
        <url-pattern>/SimplestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BeerSelect</servlet-name>
        <url-pattern>/BeerSelect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calculator</servlet-name>
        <url-pattern>/calc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Quiz</servlet-name>
        <url-pattern>/quiz</url-pattern>
    </servlet-mapping>
</web-app>